{
    "name": "easy-i18n",
    "private": true,
    "version": "0.0.9",
    "package_version": 2,
    "type": "module",
    "main": "./dist/browser.cjs",
    "scripts": {
        "dev": "npm run build -- --mode development",
        "build": "vue-tsc -b && vite build"
    },
    "devDependencies": {
        "@cocos-fe/vite-plugin-cocos-panel": "^0.0.1-beta.9",
        "@cocos/creator-types": "^3.8.6",
        "@types/node": "^22.14.0",
        "@vitejs/plugin-vue": "^5.1.2",
        "@vue/tsconfig": "^0.7.0",
        "electron": "^35.1.4",
        "rollup-plugin-node-externals": "^7.1.3",
        "typescript": "^5.8.2",
        "vite": "^5.4.1",
        "vue-tsc": "^2.0.29"
    },
    "dependencies": {
        "vue": "^3.4.37"
    },
    "contributions": {
        "menu": [
            {
                "path": "i18n:menu.extension",
                "label": "i18n:easy-i18n.title",
                "message": "open"
            }
        ],
        "messages": {
            "open": {
                "methods": [
                    "open"
                ]
            }
        },
        "asset-db": {
            "mount": {
                "path": "./assets",
                "readonly": true
            }
        },
        "builder": "./dist/builder.cjs"
    },
    "panels": {
        "default": {
            "main": "./dist/panel.cjs",
            "size": {
                "min-width": 440,
                "min-height": 740,
                "width": 700,
                "height": 740
            }
        }
    },
    "description": "i18n:easy-i18n.description",
    "author": "Yueby"
}